{
    "name": "ksdev/nbp-currency-converter",
    "description": "Retrieve average currency exchange rates from the NBP website and convert an amount from one currency to another.",
    "keywords": [
        "ksdev",
        "nbp-currency-converter"
    ],
    "homepage": "https://github.com/ksdev-pl/nbp-currency-converter",
    "license": "MIT",
    "authors": [
        {
            "name": "ksdev",
            "email": "kontakt@ksdev.pl",
            "homepage": "https://ksdev.pl",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.0",
        "guzzlehttp/guzzle": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Ksdev\\NBPCurrencyConverter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ksdev\\NBPCurrencyConverter\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    }
}
